{
  "name": "sketch-karas",
  "version": "0.0.1",
  "description": "sketch plugin",
  "maintainers": [
    {
      "name": "army8735",
      "email": "army8735@qq.com"
    }
  ],
  "engines": {
    "sketch": ">=57.0"
  },
  "skpm": {
    "name": "sketch-karas",
    "manifest": "src/manifest.json",
    "main": "sketch-karas.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "rm -rf sketch-karas.sketchplugin && skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link",
    "dev": "skpm-build --watch & skpm log -f",
    "log": "skpm log",
    "logs": "skpm log -f",
    "webpack": "webpack --color"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/karasjs/sketch-karas.git"
  },
  "dependencies": {
    "jszip": "^3.2.1",
    "karas": "^0.16.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sketch-module-web-view": "^3.4.1",
    "skpm": "^1.3.1"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.5",
    "army8735-extract": "^2.0.3",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5",
    "webstorm-disable-index": "^1.2.0"
  }
}
