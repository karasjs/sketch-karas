{
  "name": "sketch-karas",
  "version": "0.0.1",
  "description": "sketch plugin",
  "maintainers": [
    {
      "name": "army8735",
      "email": "army8735@qq.com"
    }
  ],
  "engines": {
    "sketch": ">=60.0"
  },
  "skpm": {
    "name": "sketch-karas",
    "manifest": "src/manifest.json",
    "main": "sketch-karas.sketchplugin",
    "assets": [
      "assets/**/*"
    ]
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link",
    "dev": "skpm-build --watch & skpm log -f",
    "log": "skpm log",
    "logs": "skpm log -f"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/karasjs/sketch-karas.git"
  },
  "dependencies": {
    "jszip": "^3.2.1",
    "sketch-module-web-view": "^3.4.1",
    "skpm": "^1.3.1"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4",
    "@skpm/extract-loader": "^2.0.2",
    "css-loader": "^3.2.0",
    "extract-loader": "^3.1.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "less-loader": "^5.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webstorm-disable-index": "^1.2.0"
  }
}
